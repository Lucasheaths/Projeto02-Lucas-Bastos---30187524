	$(document).ready(function(){
	  var open = false;
	  // FONTES E CORES

	  var $title_font           = "Tahoma";
	  var $title_color          = "white";
	  var $description_font     = "Tahoma";
	  var $description_color    = "white";
	  var $pagination_next      = "&gt;";
	  var $pagination_previous  = "&lt;";
	  var $pagination_color     = "white";
	  var $pagination_font      = "Tahoma";

	  function closeGallery(){
		if(open){
		  $(".stage-vc").fadeOut('fast');
		}
	  }
	  function createClicks(){
		$(".close-gallery").click(function(){
		  closeGallery();
		});

		$(".previous-vc").click(function(){
		  var $index = parseInt($(this).attr('data-goto'));
		  if($index >= 1){
			var $cam = $(`.vc-gallery img[data-index=${$index}]`).attr('src');
			var $title = $(`.vc-gallery img[data-index=${$index}]`).attr('data-title');
			var $desc = $(`.vc-gallery img[data-index=${$index}]`).attr('data-description');
			$(".stage-vc img").attr('src', $cam);
			$('.stage-vc h2').html($title);
			$('.stage-vc p.vc-desc').html($desc);

			$(this).attr('data-goto', $index-1);
			$('.next-vc').attr('data-goto', $index+1);
		  }
		});

		$(".next-vc").click(function(){
		  var $index = parseInt($(this).attr('data-goto'));
		  if($index < $size){
			var $cam = $(`.vc-gallery img[data-index=${$index}]`).attr('src');
			var $title = $(`.vc-gallery img[data-index=${$index}]`).attr('data-title');
			var $desc = $(`.vc-gallery img[data-index=${$index}]`).attr('data-description');
			$(".stage-vc img").attr('src', $cam);
			$('.stage-vc h2').html($title);
			$('.stage-vc p.vc-desc').html($desc);

			$(this).attr('data-goto', $index+1);
			$('.previous-vc').attr('data-goto', $index-1);
		  }
		});
	  }
	  var $size = 1;
	  $(".vc-gallery img").each(function(){
		$(this).attr('data-index', $size);
		$size++;
	  });
	  $(".vc-gallery img").click(function(){

		var $path = $(this).attr('src');
		var $title = $(this).attr('data-title');
		var $description = $(this).attr('data-description');

		$next_calc = parseInt($(this).attr('data-index')) + 1;
		$prev_calc = parseInt($(this).attr('data-index')) - 1;

		if($title == undefined){
		  $title = "";
		}
		if($description == undefined){
		  $description = "";
		}

		$("body").prepend(`
		<div class="stage-vc">
			<div class="modal-popup text-center" style="background-color:#f2f2f2;">
				<div class="btn btn-fechar close-gallery">X</div>
				<div class="bt-modalnextprev">
					<span data-goto="${$prev_calc}" class="previous-vc btn" style="cursor: pointer">${$pagination_previous}</span>
					<span data-goto="${$next_calc}" class="next-vc btn" style="cursor: pointer">${$pagination_next}</span>
				</div>
				<h2 style="margin-bottom:20px;">
				${$title}
				</h2>
				<img src="${$path}" alt="Alt da Imagem" title="Title da Imagem" style="max-width: 95vmin; max-height: 85vmin;margin-bottom:-13px;">
				<p class="vc-desc" style="width: 95%; text-align: center; font-family: ${$description_font}, sans-serif; color: ${$description_color}; margin-top: 15px; display: block;">
					${$description}
				</p>
			</div>
		</div>
		`);

		open = true;

		createClicks();
	  });
	  $(document).keyup(function(e){
		if(e.keyCode === 27){
		  closeGallery();
		}
	  });
	});	
	function AbasteceCidades(){
			//verifica se o browser tem suporte a ajax
		  try {
			 ajax = new ActiveXObject("Microsoft.XMLHTTP");
		  }
		  catch(e) {
			 try {
				ajax = new ActiveXObject("Msxml2.XMLHTTP");
			 }
			 catch(ex) {
				try {
				   ajax = new XMLHttpRequest();
				}
				catch(exc) {
				   alert("Esse browser não tem recursos para uso do Ajax");
				   ajax = null;
				}
			 }
		  }
		  //se tiver suporte ajax
			if(ajax) {
				document.frmEditor.strEMPCidade.options.length = 0;
				document.frmEditor.strEMPCidade.options[0] = new Option('Aguarde o carregamento...',0);
				ajax.open("GET","xml.cidades.php?estado="+document.frmEditor.strEMPEstado.value,true);
				ajax.setRequestHeader('Content-Type','application/x-www-form-urlencoded');
				ajax.send(null);
				ajax.onreadystatechange = function(){
					if(ajax.readyState==4 && ajax.status==200)
					{
					   if(ajax.responseXML) {
							processXML(ajax.responseXML);
					   }
					   else {
						   //caso não seja um arquivo XML emite a mensagem abaixo
						   alert('Por favor, selecione um Estado.');
					   }
					}
				}
			}
		}
	function processXML(obj){
		//pega a tag cidade
		var dataArray   = obj.getElementsByTagName("cidade");
		  document.frmEditor.strEMPCidade.options.length = 0;
		  //total de elementos contidos na tag cidade
		  if(dataArray.length > 0) {
			 //percorre o arquivo XML paara extrair os dados
			 for(var i = 0 ; i < dataArray.length ; i++) {
				var item = dataArray[i];
				//contéudo dos campos no arquivo XML
				var codigo    =  item.getElementsByTagName("codigo")[0].firstChild.nodeValue;
				var descricao =  item.getElementsByTagName("descricao")[0].firstChild.nodeValue;

				document.frmEditor.strEMPCidade.options.length = i;
				document.frmEditor.strEMPCidade.options[i] = new Option(descricao,codigo);
			}
		  }
		  else {
			//caso o XML volte vazio, printa a mensagem abaixo
			//idOpcao.innerHTML = "--Primeiro selecione o estado--";
		  }
	   }

	function mascara(o,f){
		v_obj=o
		v_fun=f
		setTimeout("execmascara()",1)
	}
	function execmascara(){
		v_obj.value=v_fun(v_obj.value)
	}
	function mtel(v){
		v=v.replace(/\D/g,"");             //Remove tudo o que não é dígito
		v=v.replace(/^(\d{2})(\d)/g,"($1) $2"); //Coloca parênteses em volta dos dois primeiros dígitos
		v=v.replace(/(\d)(\d{4})$/,"$1-$2");    //Coloca hífen entre o quarto e o quinto dígitos
		return v;
	}
	function id( el ){
		return document.getElementById( el );
	}
	window.onload = function(){
		if($('#telefone').length > 0) {
			id('telefone').onkeypress = function(){
				mascara( this, mtel );
			}
		} else if($('#telefone1').length > 0) {
			id('telefone1').onkeypress = function(){
				mascara( this, mtel );
			}
		} else {
			id('telefone2').onkeypress = function(){
				mascara( this, mtel );
			}
		}
	}
	$(".close-restrito").click(function () {
		$("#restrito").fadeOut();
	});
	